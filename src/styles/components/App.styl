body
  position fixed
  top 0
  right 0
  bottom 0
  left 0
  font-family 'Muli', sans-serif

p
  color #494949

h2
  margin 0

header
  background-color #def6f6
  width 100%
  display grid
  grid-template-columns 1fr 3fr
  align-items center

.Header__main img
  width 150px
  margin-left: 50px

.General__main
  background-color #9fddbd
  padding 10px
  margin 10px 0 10px 0

.Experience-cover
  display: grid
  grid-template-columns:  1fr 2fr
  width: 70%

.middle
  width: 50%
  display: flex

.Academic_Skills
  display: grid
  grid-template-columns: 1fr 1fr
  grid-column-gap 10px;

.Languages-main
  display: flex

.Languages-item
  margin-right: 60px

span
  margin-left: 150px

