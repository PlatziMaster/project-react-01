.social {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-evenly;
    margin-bottom: 16px;

    &__box {
        background-color: #00416d;
        color: #F8BD7F;
        padding: 16px;
        border-radius: 16px;
    }

    &__link {
        font-size: 18px;
        font-weight: bold;
    }
}