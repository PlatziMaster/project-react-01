@import "./Vars.styl"

.Academic
    display flex
    flex-direction: column
    gap 20px
    grid-area academic
    background secundary-color
    padding 20px
    margin-top 10px
    &-title
        color primary-color
    &-item
        background white
        border 2px solid #C7C7C7
        border-radius 10px
        padding 20px
        display flex
        gap 50px
        span
            color primary-color
            align-self center
