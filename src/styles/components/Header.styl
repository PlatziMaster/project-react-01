.Header
  display flex
  align-items center
  width 95vw    
  height 200px
  padding 15px
  background-color #B5E5FF
  border-radius 30px
  margin 15px
  .Header__image
    border 8px
    overflow hidden
    height 250px
    width 200px
    border-radius 100px
    img 
      width 200px
      height 300px

  .Header__container
    margin 20px
    .container__horizontal
      display flex
      justify-content: space-between
      h2
        margin-right 40px
