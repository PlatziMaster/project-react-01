.Header {
  width: 90%;
  height: 350px;
  margin: 10px auto 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  img {
    width: 200px;
    height: 200px;
    border-radius: 50%;
    object-fit: contain;
  }
}
.Header div {
  margin-top: 10px;
  text-align:center ;
  p, a {
    margin-top: 5px;
  }
  a {
    display: block;
    color: var(--white-color);
  }
}
.Header-title {
  font-size: 20px;
  font-weight: 500;
}