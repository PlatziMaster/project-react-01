.Experience-container 
  background-color: #7d9cff
  padding: 15px 40px
  margin-bottom: 10px
  h1 
    margin: 0
  .Experience-item-container
    display: grid;
    grid-template-columns: 1fr 1fr;
    grid-gap: 30px
    p  
      margin: 10px 0 
  .Experience-item
    margin: 20px 0 
    padding: 15px 25px
    border-radius: 20px
    box-shadow: 1px 1px 10px 0px rgba(0,0,0,0.30);
    h2, h4, p
      margin: 5px 0
      