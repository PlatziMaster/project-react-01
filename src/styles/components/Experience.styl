.Experience
  margin: 2% 0%
  width: 80%
  grid-area: experience
  display: grid
  grid-template-areas: "title" "content"
  justify-self: center
  justify-items: center
  align-items: center
  &-items
    margin-top: 2%
    grid-area: content
    -webkit-column-count: 2; /* Chrome, Safari, Opera */
    -moz-column-count: 2; /* Firefox */
    column-count: 2;
    grid-column-gap: 13%
